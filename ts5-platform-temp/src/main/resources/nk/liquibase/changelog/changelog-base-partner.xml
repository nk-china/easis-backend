<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <!--
    -->

    <changeSet id="DEF_PARTNER_ROLE-01" author="bean">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="DEF_PARTNER_ROLE" />
            </not>
        </preConditions>
        <comment />
        <createTable tableName="DEF_PARTNER_ROLE" remarks="">
            <column name="PARTNER_ROLE"         type="${type.desc}"     remarks=""><constraints primaryKey="true" /></column>
            <column name="PARTNER_ROLE_DESC"    type="${type.uuid}"     remarks="" />
            <column name="REF_OBJECT_TYPE"      type="${type.var} "     remarks="" />
            <column name="UPDATED_TIME"         type="${type.date}"     remarks="" />
            <column name="REF_DOC_TYPE"         type="${type.desc}"     remarks="" />
            <column name="STATE"             	type="${type.desc}"     remarks="" />
        </createTable>
    </changeSet>

    <changeSet id="DEF_PARTNER_ROLE_DOC-01" author="bean">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="DEF_PARTNER_ROLE_DOC" />
            </not>
        </preConditions>
        <comment />
        <createTable tableName="DEF_PARTNER_ROLE_DOC" remarks="">
            <column name="PARTNER_ROLE"         type="${type.uuid}"     remarks=""><constraints primaryKey="true" /></column>
            <column name="PRE_DOC_TYPE"         type="${type.uuid}"     remarks=""><constraints primaryKey="true" /></column>
            <column name="DOC_TYPE"             type="${type.uuid}"     remarks=""><constraints primaryKey="true" /></column>
            <column name="UPDATED_TIME"         type="${type.date}"     remarks="" />
            <column name="ORDERBY"             	type="${type.int6}"     remarks="" />
            <column name="PRE_DOC_STATUS"       type="${type.desc}"     remarks="" />
            <column name="REF_OBJECT_TYPE"      type="${type.var} "     remarks="" />
        </createTable>
    </changeSet>

    <changeSet id="BIZ_PARTNER-01" author="bean">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="BIZ_PARTNER" />
            </not>
        </preConditions>
        <comment />
        <createTable tableName="BIZ_PARTNER" remarks="">
            <column name="PARTNER_ID"           type="${type.uuid}"     remarks=""><constraints primaryKey="true" /></column>
            <column name="PARTNER_TYPE"         type="${type.desc}"     remarks="1 自然人 2 法人 3 组织机构" />
            <column name="PARTNER_NAME"         type="${type.desc}"     remarks="" />
            <column name="REMARK"             	type="${type.var} "     remarks="" />
            <column name="CREATED_TIME"         type="${type.date}"     remarks="" />
            <column name="UPDATED_TIME"         type="${type.date}"     remarks="" />
            <column name="PARTNER_TAGS"         type="${type.var} "     remarks="" />
        </createTable>
    </changeSet>

</databaseChangeLog>