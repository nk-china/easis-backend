<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <!--
    -->

    <changeSet id="SYS_USER_DASHBOARD-01" author="bean">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="SYS_USER_DASHBOARD" />
            </not>
        </preConditions>
        <comment />
        <createTable tableName="SYS_USER_DASHBOARD" remarks="">
            <column name="ID"             	type="${type.uuid}"     remarks=""><constraints primaryKey="true" /></column>
            <column name="NAME"             type="${type.desc}"     remarks="" />
            <column name="CONFIG"           type="${type.clob}"     remarks="" />
            <column name="ACCOUNT_ID"       type="${type.uuid}"     remarks="" />
            <column name="UPDATED_TIME"     type="${type.date}"     remarks="" />
            <column name="SHARED"           type="${type.int4}"     remarks="" />
        </createTable>
    </changeSet>
    <changeSet id="SYS_USER_DASHBOARD_REF-01" author="bean">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="SYS_USER_DASHBOARD_REF" />
            </not>
        </preConditions>
        <comment />
        <createTable tableName="SYS_USER_DASHBOARD_REF" remarks="">
            <column name="ID"             	type="${type.uuid}"     remarks=""><constraints primaryKey="true" /></column>
            <column name="ACCOUNT_ID"       type="${type.uuid}"     remarks="" />
            <column name="NAME"             type="${type.desc}"     remarks="" />
            <column name="ORDER_BY"         type="${type.int4}"     remarks="" />
        </createTable>
    </changeSet>

</databaseChangeLog>