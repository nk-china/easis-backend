<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <!--
    -->

    <changeSet id="DEF_WORKFLOW-01" author="bean">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="DEF_WORKFLOW" />
            </not>
        </preConditions>
        <comment />
        <createTable tableName="DEF_WORKFLOW" remarks="">
            <column name="WORKFLOW_ID"          type="${type.uuid}"     remarks="" />
            <column name="WORKFLOW_NAME"        type="${type.uuid}"     remarks="" />
            <column name="WORKFLOW_DESC"        type="${type.desc}"     remarks="" />
            <column name="UPDATED_TIME"         type="${type.date}"     remarks="" />
        </createTable>
    </changeSet>

</databaseChangeLog>